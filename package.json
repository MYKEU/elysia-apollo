    "release": "npm run build && npm run test && npm publish --access public"
  },
  "dependencies": {
    "@apollo/server-plugin-landing-page-graphql-playground": "^4.0.0",
    "@apollo/server": "4.4.1",
    "graphql": "^16.6.0",
    "graphql-tag": "^2.12.6"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.59",
    "@swc/core": "^1.3.26",
    "@types/node": "^18.11.18",
    "bun-types": "^0.5.7",
    "elysia": "^0.4.13",
    "eslint": "^8.32.0",
    "rimraf": "^4.3.1",
    "typescript": "^4.9.4"
  },
  "peerDependencies": {
    "@apollo/server-plugin-landing-page-graphql-playground": ">= 4.0.0",
    "@apollo/server": ">= 4.0.0",
    "elysia": ">= 0.3.0",
    "graphql": ">= 16.6.0",
    "graphql-tag": ">= 2.12.6"
  },
  "peerDependenciesMeta": {
    "@apollo/server-plugin-landing-page-graphql-playground": {
      "optional": true
    },
    "@apollo/server": {
      "optional": true
    },
    "graphql": {
      "optional": true
    },
    "graphql-tag": {
      "optional": true
    }
  }
